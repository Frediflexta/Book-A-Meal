{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "mocha": true
    },
     "rules": {
        "react/jsx-filename-extension": [1, {"extensions": [".js"]}]
     },
     "valid-jsdoc": ["error", {
      "requireReturn": true,
      "requireReturnType": true,
      "requireParamDescription": false,
      "requireReturnDescription": true
    }],
    "require-jsdoc": ["error", {
        "require": {
            "FunctionDeclaration": true,
            "MethodDefinition": true,
            "ClassDeclaration": true
        }
    }]
}